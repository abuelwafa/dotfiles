{
  "suggest.timeout": 5000,
  "suggest.noselect": true,
  "codeLens.enable": false,
  "coc.preferences.formatOnType": false,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.silentAutoupdate": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "json",
    "jsonc",
    "css",
    "scss",
    "html",
    "vue",
    "yaml",
    "graphql",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescript.tsx",
    "typescriptreact"
  ],

  "rest-client.enable": true,
  "rest-client.timeoutinmilliseconds": 40000,

  "suggest.snippetIndicator": "~",
  "suggest.snippetsSupport": true,
  "snippets.triggerCharacters": ["kj"],
  "snippets.loadFromExtensions": false,
  "snippets.ultisnips.enable": true,
  "snippets.autoTrigger": true,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "snippets.shortcut": "...",
  "snippets.priority": 9999,

  "eslint.autoFixOnSave": false,
  "eslint.run": "onSave",

  "typescript.suggest.paths": true,
  "typescript.validate.enable": false,
  "typescript.implementationsCodeLens.enable": false,
  "typescript.format.enabled": false,
  "typescript.referencesCodeLens.enable": false,
  "typescript.suggestionActions.enabled": false,

  "prettier.requireConfig": true,
  "prettier.printWidth": 100,
  "prettier.tabWidth": 4,
  "prettier.useTabs": false,
  "prettier.endOfLine": "lf",
  "prettier.trailingComma": "all",
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.bracketSameLine": false,
  "prettier.jsxSingleQuote": false,
  "prettier.arrowParens": "avoid",
  "prettier.bracketSpacing": true,
  "prettier.statusItemText": "",
  "prettier.disableLanguages": [],
  "prettier.onlyUseLocalVersion": true,
  "prettier.enable": true,

  "rust.build_on_save": true,
  "rust-client.updateOnStartup": true,

  "go.checkForUpdates": "inform",
  "clangd.checkUpdates": true
}
